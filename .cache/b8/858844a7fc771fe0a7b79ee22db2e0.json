     transition: clip-path 0.5s cubic-bezier(0.455, 2.5, 0.645, 0.55);\r\n    }\r\n    &:hover:before{\r\n        \r\n        \r\n        clip-path: path(\"M143,77 C117,96 74,100.5 45.5,91.5 C17,82.5 -10.5,57 5.5,31.5 C21.5,6 79,-5.5 130.5,4 C182,13.5 169,58 143,77 Z\");\r\n    }\r\n    & span {\r\n        mix-blend-mode: difference;\r\n        color: $green-color;\r\n        \r\n    }\r\n    &:after{\r\n        content: \"\";\r\n        height: 85%;\r\n        width: 95%;\r\n        top: 6px;\r\n        border: 1px solid $green-color;\r\n        border-radius: 58% 42% 55% 45% / 56% 45% 55% 44%;\r\n        transform: rotate(-20deg);\r\n        transition: transform 0.5s cubic-bezier(0.455, 2.5, 0.645, 0.55);\r\n    }\r\n    &:hover:after{\r\n        transform: translate3d(0, -5px, 0);\r\n    }\r\n    @media (max-width: 800px){\r\n        &{\r\n            transform: scale3d(0.7,0.7,0.7);\r\n            font-size: 23px;\r\n        }\r\n    }\r\n}\r\n\r\n.uil{\r\n    font-size: 35px;\r\n    text-shadow: -10px -20px $black-color;\r\n    &:hover{\r\n        animation: coloredAnimation 0.005s infinite;\r\n    }\r\n    \r\n    @keyframes coloredAnimation {\r\n        0%{\r\n            text-shadow: 5px 4px $green-color,5px 4px $black-color;\r\n        }\r\n        25%{\r\n            text-shadow: -5px -6px $green-color,-5px -6px $black-color;\r\n        }\r\n        50%{\r\n            text-shadow: 5px -4px $green-color,5px -4px $black-color;\r\n        }\r\n        75%{\r\n            text-shadow: -8px -4px $green-color,-5px -4px $black-color;\r\n        }\r\n        100%{\r\n            text-shadow: -5px 0px $green-color,5px -6px $black-color;\r\n        }\r\n        \r\n    }\r\n}\r\n.coolEye{\r\n    position: relative;\r\n    display: inline-block;\r\n    top: 100px;\r\n    .textcircle{\r\n        display: block;\r\n        width: 250px;\r\n        animation: roatte 10s linear infinite;\r\n        text{\r\n            font-size: 32px;\r\n            text-transform: uppercase;\r\n            fill: $green-color;\r\n        }\r\n    }\r\n    @keyframes roatte {\r\n        to{\r\n            transform: rotate(-360deg);\r\n        }\r\n    }\r\n}\r\n.eye{\r\n    position: absolute;\r\n    width: 60px;\r\n    height: 60px;\r\n    top: 50%;\r\n    left: 50%;\r\n    transform: translate(-50%,-50%);\r\n    &__outer,&__inner,&__lashes-up,&__lashes-down{\r\n        stroke: $green-color;\r\n        fill: none;\r\n        stroke-width: 1.5px;\r\n    }\r\n    &__lashes-down{\r\n        opacity: 0;\r\n    }\r\n}","assets\\styles\\projects.scss":"@import '../styles/base.scss';\r\n@import '../styles/coolinks.scss';\r\n.projects{\r\n    font-family: 'File Deletion', sans-serif;\r\n    color: $green-color;\r\n    &__header{\r\n        font-size: 40px;\r\n        font-style: italic;\r\n        padding-bottom: 10px;\r\n\r\n    }\r\n}\r\n\r\n.project{\r\n    font-family: 'File Deletion', sans-serif;\r\n    color: $green-color;\r\n    &__header{\r\n        display: grid;\r\n        grid-template-columns: 1fr 3fr;\r\n        gap: 1rem;\r\n        span{\r\n            border-top: 1px solid $green-color;\r\n            padding: 1rem;\r\n            font-size: 25px;\r\n            text-align: center;\r\n        }\r\n    }\r\n    &__infos{\r\n        margin-top: 1rem;\r\n        &--name{\r\n            position: relative;\r\n            font-size: 40px;\r\n            display: flex;\r\n            align-items: center;\r\n            font-weight: 400;\r\n        }\r\n    }\r\n    &__img{\r\n        position: relative;\r\n        width: 100%;\r\n        height: 700px;\r\n        img{\r\n            width: 60vw;\r\n            height: 600px;\r\n            object-fit: cover;\r\n            position: absolute;\r\n            right: 15vw;\r\n            top: 3rem;\r\n        }\r\n    }\r\n    &__links{\r\n        padding-top: 3rem;\r\n        button{\r\n            bottom: 170px;\r\n            left: 20px;\r\n        }\r\n        \r\n    }\r\n\r\n    &__tags{\r\n        position: absolute;\r\n        bottom: 1rem;\r\n        left: 30px;\r\n        display: flex;\r\n        flex-direction: column;\r\n        gap: 1rem;\r\n    }\r\n    @media (max-width: 1350px){\r\n        .projects{\r\n            padding: 3rem;\r\n        }\r\n        .project__img{\r\n            width: 74vw;\r\n            right: 5rem;\r\n        }\r\n        .project__info--name{\r\n            font-size: 25px;\r\n        }\r\n        .project .coolEyebutton{\r\n            transform: translate(-14rem, -3rem);\r\n        }\r\n    }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"f6cecf4a4c509160883d8b2516aa859e","cacheData":{"env":{}}}